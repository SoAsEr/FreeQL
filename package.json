{
  "name": "freeql",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://SoAsEr.github.io/FreeQL",
  "dependencies": {
    "@popperjs/core": "^2.7.2",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@tippyjs/react": "^4.2.0",
    "classnames": "^2.2.6",
    "comlink": "^4.3.0",
    "deepmerge": "^4.3.1",
    "fast-memoize": "^2.5.2",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "graphql": "^16.6.0",
    "is-number": "^7.0.0",
    "jsonpointer": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.2.0",
    "react-popper": "^2.2.4",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.3",
    "react-window": "^1.8.6"
  },
  "devDependencies": {
    "ajv": "^8.2.0",
    "ajv-cli": "^5.0.0",
    "ajv-errors": "^3.0.0",
    "ajv-keywords": "^5.0.0",
    "autoprefixer": "^10.2.4",
    "color": "^4.2.3",
    "eslint-plugin-vuln-regex-detector": "^1.0.4",
    "gh-pages": "^5.0.0",
    "html-to-react": "^1.4.5",
    "node-html-parser": "^6.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^10.1.0",
    "preval.macro": "^5.0.0",
    "source-map-explorer": "^2.5.2",
    "tailwind-children": "^0.5.0",
    "tailwindcss": "^3.3.2",
    "worker-loader": "^3.0.8"
  },
  "babelMacros": {
    "preval": {
      "pure": true
    }
  },
  "scripts": {
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "test:regex": "eslint --plugin vuln-regex-detector --rule '\"vuln-regex-detector/no-vuln-regex\": 2' ./src/reusable_components/formatting/FormattedChemicalCompound.js",
    "build:assembly": "node public/static/js/build.js",
    "build:tailwind": "npx tailwindcss -i ./src/tailwind.css -o src/tailwind.output.css",
    "build:ajv-form": "ajv compile --spec=draft2020 --data --all-errors --code-es5 -s src/AJV/equilibriumform.json -r src/AJV/equilibriumdata.json -c ajv-errors -c ajv-keywords -c ./src/AJV/custom_keywords.js -o ./src/validation/validate_eq_form.js",
    "build:ajv-data": "ajv compile --all-errors --code-es5 -s src/AJV/equilibriumdata.json -c ajv-keywords -c ./src/AJV/custom_keywords.js -o ./src/validation/validate_eq_data.js",
    "build:ajv": "npm-run-all --parallel build:ajv-form build:ajv-data",
    "watch": "node watch.js",
    "watch:tailwind": "npx tailwindcss -i ./src/tailwind.css -o src/tailwind.output.css --watch",
    "start:react": "react-scripts start",
    "start": "npm-run-all --parallel build:assembly build:ajv build:tailwind --parallel watch:tailwind start:react",
    "build:react": "react-scripts build",
    "build": "npm-run-all --parallel build:tailwind build:assembly build:ajv --sequential build:react",
    "predeploy": "npm run build --prod",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
